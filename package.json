{
  "name": "sqal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "roboto-fontface": "*",
    "vue": "^3.2.41",
    "vuestic-ui": "^1.4.10",
    "vuetify": "npm:@vuetify/nightly@next",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@iconify/json": "^2.1.130",
    "@unocss/preset-icons": "^0.46.1",
    "@unocss/preset-tagify": "^0.46.1",
    "@unocss/preset-web-fonts": "^0.46.1",
    "@unocss/reset": "^0.46.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate": "^2.3.0",
    "prettier": "2.7.1",
    "sass": "^1.55.0",
    "unocss": "^0.46.0",
    "vite": "^3.2.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2021": true,
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "import",
      "html",
      "vue",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 120,
          "singleQuote": true,
          "singleAttributePerLine": true,
          "trailingComma": "all",
          "bracketSpacing": true,
          "arrowParens": "always"
        }
      ],
      "vue/valid-define-emits": 0,
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": {
            "max": 2
          },
          "multiline": {
            "max": 1
          }
        }
      ]
    }
  },
  "type": "module"
}
